{
  "name": "reach4help-site",
  "private": true,
  "version": "2.5.2",
  "main": "index.js",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "build:dev": "gatsby clean && gatsby build",
    "build:staging": "gatsby clean && gatsby build",
    "build:prod": "gatsby clean && gatsby build",
    "develop": "gatsby clean && GATSBY_SITE_URL=http://localhost:8000 gatsby develop",
    "start": "yarn develop",
    "serve": "yarn build && gatsby serve -p 9000",
    "lint": "eslint ./src && prettier --check \"./src/**/*.{js,jsx}\"",
    "lint:fix": "eslint ./src --fix && prettier --write \"./src/**/*.{js,jsx}\"",
    "setup": "node ./bin/setup.js",
    "e2e:open": "cypress open",
    "e2e:ci": "DETERMINISTIC=true start-server-and-test serve 9000 visual-test",
    "visual-test": "percy exec -t 350 -- cypress run",
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/covidaidapp/covidaid/tree/master/site.git"
  },
  "keywords": [
    "gatsby",
    "reach4help",
    "covid",
    "PWA"
  ],
  "license": "MIT",
  "dependencies": {
    "@tippy.js/react": "^3.1.1",
    "dotenv": "^8.2.0",
    "envfile": "^4.5.0",
    "gatsby": "^2.20.8",
    "gatsby-plugin-google-analytics": "^2.1.34",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-netlify-cms": "^4.2.0",
    "gatsby-plugin-netlify-identity-widget": "^1.1.1",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-preload-link-crossorigin": "^1.0.2",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-styled-components": "^3.1.18",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.6.53",
    "inquirer": "^7.0.4",
    "netlify-cms": "^2.10.35",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-headroom": "^3.0.0",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.9.0",
    "react-markdown": "^4.3.1",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^1.3.0",
    "rebass": "^3.0.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@percy/cypress": "^2.1.1",
    "axe-core": "^3.4.1",
    "chalk": "^3.0.0",
    "contentful-import": "^7.4.0",
    "cypress": "^3.8.3",
    "cypress-axe": "^0.5.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.18.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.10.8"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  }
}
